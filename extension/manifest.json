{
  "name": "FrameBuster",
  "description": "Better package manifests on Github",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}